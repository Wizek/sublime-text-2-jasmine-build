{
    "name": "JasmineConsole",
    "patterns": [
        {
            "match": "\\d+ test(s*)|PASS",
            "name": "test.pass"
        },
        {
            "match": "Expected (.+) to (be|have) ([^.]+)",
            "name": "test.error_help",
             "captures": {
              "1": {"name": "test.error"},
              "2": {"name": "test.error_help"},
              "3": {"name": "test.error"}
            }
        },
        {
            "match": "\\d+ failure(s*)|FAIL",
            "name": "test.error"
        }
    ],
    "scopeName": "console"
}